{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "father_name": {
      "type": "string"
    },
    "mobile": {
      "type": "integer"
    },
    "dob": {
      "type": "date"
    },
    "skills": {
      "type": "json"
    },
    "hobbies": {
      "type": "json"
    },
    "youtubes": {
      "type": "json"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "profiles"
    },
    "hostel_name": {
      "type": "string"
    },
    "room_number": {
      "type": "integer"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "degree": {
      "type": "enumeration",
      "enum": [
        "BE/BTECH",
        "ME/MTECH",
        "MSC",
        "BSC",
        "MBA/PGDM",
        "BPHARMA",
        "MPHARMA",
        "BBALLB",
        "BDES",
        "BALLB",
        "PHD",
        "PGDIPLOMA",
        "MPHIL"
      ]
    },
    "semester": {
      "type": "enumeration",
      "enum": [
        "I",
        "II",
        "III",
        "IV",
        "V",
        "VI",
        "VII",
        "VIII"
      ]
    },
    "branch": {
      "type": "string"
    },
    "userrole": {
      "type": "enumeration",
      "enum": [
        "STUDENT",
        "GARDIAN"
      ]
    },
    "ismobilevisible": {
      "type": "boolean"
    },
    "campus": {
      "type": "enumeration",
      "enum": [
        "GOA",
        "HYDRABAD",
        "MUMBAI",
        "PILANI",
        "DUBAI"
      ]
    },
    "adresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::adress.adress",
      "mappedBy": "profile"
    }
  }
}
