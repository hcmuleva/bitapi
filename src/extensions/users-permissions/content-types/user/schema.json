{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "MALE",
        "FEMALE"
      ]
    },
    "hobbies": {
      "type": "json"
    },
    "languages": {
      "type": "json"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "user"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "users"
    },
    "userrole": {
      "type": "enumeration",
      "enum": [
        "STUDENT",
        "GARDIAN"
      ]
    },
    "profession": {
      "type": "string"
    },
    "specilization": {
      "type": "string"
    },
    "orgrole": {
      "type": "string"
    },
    "orgname": {
      "type": "string"
    },
    "semester": {
      "type": "string"
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "users_permissions_user"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "hostel_name": {
      "type": "string"
    },
    "room_number": {
      "type": "string"
    },
    "degree": {
      "type": "enumeration",
      "enum": [
        "BE/BTECH",
        "ME/MTECH",
        "MSC",
        "BSC",
        "MBA/PGDM",
        "BPHARMA",
        "MPHARMA",
        "BBALLB",
        "BDES",
        "BALLB",
        "PHD",
        "PGDIPLOMA",
        "MPHIL"
      ]
    },
    "campus": {
      "type": "enumeration",
      "enum": [
        "GOA",
        "HYDRABAD",
        "MUMBAI",
        "PILANI",
        "DUBAI"
      ]
    },
    "branch": {
      "type": "string"
    },
    "mobile": {
      "type": "integer"
    },
    "joblocation": {
      "type": "string"
    },
    "isNoMobile": {
      "type": "boolean"
    },
    "isNoAddress": {
      "type": "boolean"
    },
    "isNoEmail": {
      "type": "boolean"
    },
    "myideas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::myidea.myidea",
      "mappedBy": "participants"
    },
    "travels": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::travel.travel",
      "mappedBy": "user"
    },
    "travelers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::travel.travel",
      "mappedBy": "participants"
    },
    "mytravels": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mytravel.mytravel",
      "mappedBy": "traveledby"
    },
    "passenger": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::mytravel.mytravel",
      "mappedBy": "participants"
    },
    "bloodgroup": {
      "type": "string"
    }
  }
}
